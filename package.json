    "demo": "vite playground",
    "vercel-build": "npm run build:playground"
  },
  "keywords": [
    "hover",
    "effects",
    "image",
    "animation",
    "canvas",
    "typescript",
    "ascii",
    "zoom",
    "particle",
    "pixel",
    "minecraft",
    "lego"
  ],
  "author": "Harsh Singhal",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.38.0",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.0.4",
    "vite": "^4.3.0",
    "vitest": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hsrambo07/hover-effects"
  },
  "bugs": {
    "url": "https://github.com/hsrambo07/hover-effects/issues"
  },
